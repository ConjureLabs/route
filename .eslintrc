{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "rules": {
    "arrow-spacing": [2, {
      "before": true,
      "after": true
    }],
    "brace-style": [2, "1tbs", {
      "allowSingleLine": false
    }],
    comma-dangle: [2, "never"],
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }],
    "comma-style": [2, "last"],
    "curly": [2, "all"],
    "dot-notation": 2,
    "handle-callback-err": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-semi": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-compare": 2,
    "no-sparse-arrays": 2,
    "no-sync": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {
      "varsIgnorePattern": "^[A-Z]"
    }],
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "semi": [2, "always"],
    "valid-typeof": 2
  }
}
